{
  "name": "idl-website",
  "version": "1.0.0",
  "description": "eleventy based static site for interaction design lab at university of applied sciences potsdam",
  "main": "index.js",
  "scripts": {
    "clean": "del-cli 'dist'",
    "start": "npm run clean && npm run dev",
    "dev": "npm run clean && cross-env ELEVENTY_ENV=dev eleventy --serve",
    "build": "npm run clean && cross-env ELEVENTY_ENV=prod eleventy",
    "seed": "npm run clean && cross-env ELEVENTY_ENV=seed eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FH-Potsdam/IDL-Website.git"
  },
  "author": "sebastian-meier",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/FH-Potsdam/IDL-Website/issues"
  },
  "homepage": "https://github.com/FH-Potsdam/IDL-Website#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "cross-env": "7.0.3",
    "cssnano": "^5.1.13",
    "del-cli": "^5.0.0",
    "esbuild": "^0.16.10",
    "esbuild-plugin-manifest": "^0.5.0",
    "fast-xml-parser": "^4.0.9",
    "html-minifier": "^4.0.0",
    "json2yaml": "^1.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "markdown-it-implicit-figures": "^0.10.0",
    "moment": "^2.29.4",
    "postcss": "^8.4.16",
    "postcss-color-mix": "^1.1.0",
    "postcss-import": "^15.0.0",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^5.0.6",
    "postcss-normalize": "^10.0.1",
    "precss": "^4.0.0",
    "uglify-js": "^3.17.0"
  },
  "dependencies": {
    "gsap": "^3.11.4"
  }
}
