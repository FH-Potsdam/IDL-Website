    "cross-env": "5.2.0",
    "fast-xml-parser": "^4.0.1"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "cssnano": "^5.0.15",
    "html-minifier": "^4.0.0",
    "moment": "^2.29.1",
    "postcss": "^8.4.5",
    "postcss-color-mix": "^1.1.0",
